<p>
  <a href="/transactions/add_transaction %>" class="btn btn-success">Click here to add a transaction!</a>
  <% db=0 %>
    <% if (transactions.length > 0) { %>
        <% transactions.forEach(transactions => { %>
          <div class="card mt-4">
            <div class="card-body">
              <h4 class="card-title">
                <!-- <%= transactions.Date %>  -->
                 <%= transactions._symbol[0].Description %> 
                
                 <!-- <%= prices[db] %> -->
                 </h4>
              <div class="card-text mb-2">
                <% if (result[db] > 0) { %>
                <h4>Value of the position:  +<%= result[db] %> %  </h4>
                <% } else if (result[db] < 0){ %>
                <h4>Value of the position:  <%= result[db] %> % </h4>
                <% } else { %>
                  <h4>Value of the position:  +<%= result[db] %> %  </h4>
                  <% } %>
                
                </div>
                
                <!-- quantity= piece -->
                
                <a href="/transactions/<%= transactions._symbol[0].Symbol %>" class="btn btn-success">Read More</a>
                <!-- <a href="/asd/<%= transactions.id %>" class="btn btn-warning">Edit</a> -->
                <!-- <form action="/articles/<%= transactions.id %>?_method=DELETE" method="POST" class="d-inline">
                  <button type="submit" class="btn btn-danger">Delete</button>
                </form> -->
              </div>
            </div>
            <% db=db+1 %>
  <% })}else {%>
    <h4>You don't have any transactions! 
    </h4>
    <a href="/transactions/add_transaction %>" class="btn btn-success">Click here to add a transaction!</a>
    <% }%>
 </p>