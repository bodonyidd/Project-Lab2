        table.showcase:after {
            content: "";
            display: block;
            position: absolute;
            opacity: 0.5;
            left: 0;
            right: 0;
            bottom: 20px;
            margin: auto;
            height: 0;
            width: 0;
            border-width: 18px;
            border-style: solid;
            border-color: transparent;
            border-top-color: inherit;
        }

        table.showcase.last-showcase:after {
            display: none;
        }

        table.showcase .showcase-inner h1 {
            font-size: 50px;
            text-align: center;
            font-weight: 300;
        }

        table.showcase .showcase-inner h2 {
