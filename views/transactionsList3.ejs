            font-family: inherit;
            font-size: 16px;
        }

        table.showcase.hero table[data-sortable].sortable-theme-bootstrap th[data-sorted="true"] {
            color: #0daf77;
            background: #b8f1dd;
            border-bottom-color: #22bd88;
        }

        table.showcase .showcase-inner table[data-sortable] {
            margin: 4em auto 3em;
